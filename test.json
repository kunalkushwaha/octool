{
	"version": "0.1",
	"platform": {
		"os": "linux",
		"arch": "amd64"
	},
	"processes": [
	    {
	        "tty": true,
	        "user": "daemon",
	        "args": [
	            "sh"
	        ],
	        "env": [
	            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
	            "TERM=xterm"
	        ],
	        "cwd": ""
	    }
	],
	"root": {
	    "path": "rootfs",
	    "readonly": true
	},
	"hostname": "shell",
	"mounts": [
	    {
	        "type": "proc",
	        "source": "proc",
	        "destination": "/proc",
	        "options": ""
	    },
	    {
	        "type": "tmpfs",
	        "source": "tmpfs",
	        "destination": "/dev",
	        "options": "nosuid,strictatime,mode=755,size=65536k"
	    },
	    {
	        "type": "devpts",
	        "source": "devpts",
	        "destination": "/dev/pts",
	        "options": "nosuid,noexec,newinstance,ptmxmode=0666,mode=0620,gid=5"
	    },
	    {
	        "type": "tmpfs",
	        "source": "shm",
	        "destination": "/dev/shm",
	        "options": "nosuid,noexec,nodev,mode=1777,size=65536k"
	    },
	    {
	        "type": "mqueue",
	        "source": "mqueue",
	        "destination": "/dev/mqueue",
	        "options": "nosuid,noexec,nodev"
	    },
	    {
	        "type": "sysfs",
	        "source": "sysfs",
	        "destination": "/sys",
	        "options": "nosuid,noexec,nodev"
	    }
	]
}
